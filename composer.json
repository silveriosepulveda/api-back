{
  "name": "silveriosepulveda/classegeral",
  "description": "Classe de Gerenciamento de Dados em PHP",
  "type": "library",
  "require": {
    "php": "^7.2 || ^8.0",
    "fpdf/fpdf": "^1.86",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-gd": "*",
    "ext-curl": "*",
    "ext-iconv": "*",
    "phpoffice/phpspreadsheet": "^4.4",
    "slim/slim": "^4.14",
    "slim/psr7": "^1.7",
    "mpdf/mpdf": "^8.2",
    "phpmailer/phpmailer": "^7.0",
    "mercadopago/dx-php": "^3.8"

  },
  "autoload": {
    "psr-4": {
      "ClasseGeral\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ClasseGeral\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Siverio Sepulveda",
      "email": "silveriosepulveda@yahoo.com.br"
    }
  ],
  "homepage": "https://github.com/silveriosepulveda/classeGeral",
  "keywords": [
    "php",
    "class",
    "library"
  ]
}
